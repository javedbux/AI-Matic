{
  "name": "aicodeassistant",
  "displayName": "AICodeAssistant",
  "description": "Code Assistant using Ollama like Git Co-Pilot",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
   "contributes": {
    "commands": [
      {
        "command": "codeninja.helloWorld",
        "title": "Code-X - Hello World"
      },
      {
        "command": "codeninja.codeExplain",
        "title": "Code-X - Explain Code"
      },
      {
        "command": "codeninja.fixCode",
        "title": "Code-X - Fix Code"
      },
      {
        "command": "codeninja.codetranslate",
        "title": "Code-X - Code and Translate Code"
      },
      {
        "command": "codeninja.chatBox",
        "title": "Code-X - Ask Code"
      },
      {
        "command": "codeninja.generatecode",
        "title": "Code-X - Generate Code from a Comment"
      },
      {
        "command": "codeninja.refactorcode",
        "title": "Code-X - Code Refactor"
      },
      {
        "command": "codeninja.hellogemini",
        "title": "Code-X - Gemini AI for option, show message"
      },
      {
        "command": "codeninja.openSettings",
        "title": "Code-X - Open Settings"
      }
    ], "menus": {
      "editor/context": [
        {
          "command": "codeninja.codeExplain",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "codeninja.fixCode",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "codeninja.helloWorld",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "codeninja.hellogemini",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "codeninja.chatBox",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    },"configuration": {
      "title": "AI Assistant Settings",
      "properties": {
        "aiAssistant.apiUrl": {
          "type": "string",
          "default": "http://localhost:11434/api/generate",
          "description": "The base URL for the AI API (e.g., Ollama, OpenAI, or custom)."
        },
        "aiAssistant.model": {
          "type": "string",
          "default": "phi3",
          "description": "The model name to use (e.g., llama3.1, mistral, gpt-4)."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "ts-loader": "^9.5.4",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
